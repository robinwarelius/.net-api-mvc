@model CollectionModel
@{
    ViewBag.Title = "Sorted Products";
}


<section>
    <div class="container user-input-links-item">      
        <a asp-controller="Home" asp-action="Index" class="user-input-link">Home > </a>
        <span class="user-input-text">@Model.Title</span>
    </div>

    <div class="product-title">@ViewBag.Category Products</div>
    <partial name="~/Views/Shared/Partials/_CollectionPartial.cshtml" model="Model"></partial>
</section>